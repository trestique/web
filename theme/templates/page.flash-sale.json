    "what_makes": {
      "type": "what-makes-it-trestique",
      "disabled": true,
      "blocks": {
        "3423c95d-206e-480a-8c23-8bbda24ee214": {
          "type": "Slide",
          "settings": {
            "slide_img": "shopify:\/\/shop_images\/travel-friendly.png",
            "slide_title": "TRAVEL-FRIENDLY",
            "slide_text": "Light-as-air makeup kits weigh less than 1\/2 lb and are TSA-approved. Our sleek makeup bags fit your entire beauty routine and easily slides into your carry-on without taking up space."
          }
        },
        "00e26c1a-fd65-4586-9c90-dd7df1b6834d": {
          "type": "Slide",
          "settings": {
            "slide_img": "shopify:\/\/shop_images\/easy-to-carry.png",
            "slide_title": "EASY APPLICATION",
            "slide_text": "Mistake-proof makeup sticks are easy to apply and spill-proof. All makeup bags feature a built-in mirror, some with a multi-purpose carrying strap."
          }
        },
        "eea651ca-fb7e-4068-87b1-5a85347cc870": {
          "type": "Slide",
          "settings": {
            "slide_img": "shopify:\/\/shop_images\/product-tool.png",
            "slide_title": "SMART 2-IN-1 DESIGN",
            "slide_text": "Every full size product comes with a built-in tool, applicator or formula. No matter where you are in the world, application is seamless and touch-ups are a breeze."
          }
        },
        "817aa1aa-fcb2-42d1-bef7-0952d74a4197": {
          "type": "Slide",
          "settings": {
            "slide_img": "shopify:\/\/shop_images\/time-saving.png",
            "slide_title": "TIME-SAVING",
            "slide_text": "Meet your full face in a case that applies in 5 minutes or less. Our functional beauty routines have everything you need and nothing you don't, so you have one less thing to worry about packing."
          }
        },
        "3654e456-8228-4ac4-be43-9c949dfd59d4": {
          "type": "Slide",
          "settings": {
            "slide_img": "shopify:\/\/shop_images\/endlessly.png",
            "slide_title": "SUSTAINABLY REFILLABLE",
            "slide_text": "We're sustainable as beauty gets. Our makeup products are endlessly refillable, made from recycled materials, certified carbon neutral by CarbonFund and are easy to recycle."
          }
        },
        "c4e48768-8e2a-4d1a-b9cf-dff5f520671e": {
          "type": "Slide",
          "settings": {
            "slide_img": "shopify:\/\/shop_images\/CLEAN_668dd7f2-1e85-4da5-9298-d8ed70d8bbf8.png",
            "slide_title": "CLEAN FORMULAS",
            "slide_text": "PETA-certified, cruelty-free and vegan formulas. Our high-performance makeup features cream-gel textures that doesn't sweat on your skin or look cakey."
          }
        },
        "ae1a3455-1fac-419d-b34b-f55e2d035a8b": {
